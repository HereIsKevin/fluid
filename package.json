{
  "name": "@hereiskevin/fluid",
  "version": "0.4.0",
  "description": "Simple, lightweight, and blazing fast UI library",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "shx rm -rf ./build/ ./dist/",
    "watch:tsc": "tsc --watch",
    "watch:rollup": "rollup --watch --silent --config ./rollup.config.js",
    "watch": "npm-run-all --parallel watch:tsc watch:rollup",
    "build:tsc": "tsc --declaration --declarationDir ./dist/",
    "build:rollup": "rollup --config ./rollup.config.js",
    "build:terser": "terser --config-file ./terser.json --output ./dist/index.min.js ./dist/index.js",
    "build": "npm-run-all --sequential build:tsc build:rollup build:terser",
    "format": "prettier --write ./src/ ./*.json ./*.js ./*.cjs ./*.md",
    "lint": "eslint --fix --ext .ts ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HereIsKevin/fluid.git"
  },
  "keywords": [
    "simple",
    "lightweight",
    "fast",
    "ui",
    "templating",
    "framework",
    "virtual",
    "dom",
    "minimal",
    "template"
  ],
  "author": "Kevin Feng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HereIsKevin/fluid/issues"
  },
  "homepage": "https://github.com/HereIsKevin/fluid#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "npm-run-all": "^1.7.0",
    "prettier": "^2.2.1",
    "rollup": "^2.47.0",
    "shx": "^0.3.3",
    "terser": "^5.7.0",
    "typescript": "^4.2.4"
  }
}
